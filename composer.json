{
    "name": "zikula/permissions-module",
    "version": "3.1.0-dev",
    "description": "User permissions manager",
    "type": "zikula-system-module",
    "license": "LGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Zikula",
            "homepage": "https://ziku.la/"
        }
    ],
    "autoload": {
        "psr-4": { "Zikula\\PermissionsModule\\": "" }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/contracts": "^5.2",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/security": "^5.2",
        "symfony/validator": "^5.2",

        "zikula/core-bundle": "self.version",
        "zikula/coreinstaller-bundle": "self.version",
        "zikula/formextension-bundle": "self.version",
        "zikula/extensions-module": "self.version",
        "zikula/groups-module": "self.version",
        "zikula/menu-module": "self.version",
        "zikula/permissions-module": "self.version",
        "zikula/theme-module": "self.version",
        "zikula/users-module": "self.version"
    },
    "require-dev": {
        "symfony/translation": "^5.2"
    },
    "extra": {
        "zikula": {
            "class": "Zikula\\PermissionsModule\\ZikulaPermissionsModule",
            "core-compatibility": ">=3.0.0",
            "displayname": "Permissions",
            "url": "permissions",
            "icon": "fas fa-key",
            "capabilities": {
                "admin": {
                    "route": "zikulapermissionsmodule_permission_listpermissions"
                }
            },
            "securityschema": {
                "ZikulaPermissionsModule::": "::"
            }
        }
    }
}
